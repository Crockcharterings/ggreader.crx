{
   "content_scripts": [ {
      "css": [ "greader.css" ],
      "js": [ "greader.js" ],
      "matches": [ "*://www.feedly.com/*", "*://*.feedly.com/*", "*://feedly.com/*" ],
      "run_at": "document_end"
   } ],
